{
  "name": "schemats",
  "version": "3.0.3",
  "description": "Generate typescript interface definitions from (postgres) SQL database schema",
  "keywords": [
    "postgres",
    "schema",
    "typescript",
    "sql"
  ],
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "private": true,
  "scripts": {
    "build:watch": "npx tsc -w",
    "nps": "nps"
  },
  "bin": {
    "schemats": "bin/schemats.js"
  },
  "dependencies": {
    "case": "1.6.3",
    "diff": "3.2.0",
    "dotenv": "8.2.0",
    "lodash": "4.17.4",
    "mysql": "2.13.0",
    "mz": "2.6.0",
    "nps": "5.10.0",
    "pg-promise": "6.3.6",
    "pluralize": "8.0.0",
    "typescript": "3.9.5",
    "typescript-formatter": "7.0.1",
    "yargs": "8.0.1"
  },
  "devDependencies": {
    "@types/diff": "3.2.0",
    "@types/lodash": "4.14.37",
    "@types/mysql": "0.0.33",
    "@types/mz": "0.0.31",
    "@types/node": "6.0.77",
    "@types/pluralize": "0.0.29",
    "@types/power-assert": "1.4.29",
    "@types/proxyquire": "1.3.27",
    "@types/yargs": "6.3.3",
    "coveralls": "2.11.15",
    "del-cli": "0.2.0",
    "dependency-check": "2.6.0",
    "nyc": "11.0.2",
    "power-assert": "1.4.2",
    "prettier": "2.1.2",
    "proxyquire": "1.7.11",
    "source-map-support": "0.4.15",
    "ts-node": "3.0.4",
    "tslint": "5.4.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "5.0.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
